{
	"extends": [
		"next/core-web-vitals",
		"eslint:recommended",
		"plugin:import/recommended",
		"plugin:import/warnings",
		"prettier"
	],
	"plugins": ["unused-imports"],
	"rules": {
		"import/no-unresolved": "off",
		"no-unused-vars": "off",
		"no-useless-escape": "off",
		"@next/next/no-img-element": "off"
	}
}
